-- ðŸ”’ Username Lock + Multi-Loader
local allowedUsers = {
    ["6atotkaca9"]   = true,
    ["9wenerd"]      = true,
    ["AsepSlebewNie"]= true,
    ["ShedowBlack7"] = true,
}

local Players = game:GetService("Players")
local lp = Players.LocalPlayer

if not allowedUsers[lp.Name] then
    warn(("Access Denied for '%s'"):format(lp.Name))
    return
end

-- helper aman untuk fetch + jalankan
local function fetch(url)
    local ok, res = pcall(function()
        return game:HttpGet(url, true)
    end)
    if not ok then
        error(("Gagal ambil URL:\n%s\nDetail: %s"):format(url, tostring(res)))
    end
    return res
end

local function run(src, url)
    local f, err = loadstring(src)
    if not f then
        error(("Gagal compile dari URL:\n%s\nDetail: %s"):format(url, tostring(err)))
    end
    local ok, runtimeErr = pcall(f)
    if not ok then
        error(("Error saat eksekusi dari URL:\n%s\nDetail: %s"):format(url, tostring(runtimeErr)))
    end
end

-- daftar script yang ingin di-load
local urls = {
    "https://raw.githubusercontent.com/aditpacked/ramah-kali-we/refs/heads/main/ahaygnungrmah",
    "https://raw.githubusercontent.com/aditpacked/ramah-kali-we/refs/heads/main/versitombol",
}

for _, url in ipairs(urls) do
    local src = fetch(url)
    run(src, url)
end